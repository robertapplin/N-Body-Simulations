<?xml version = "1.0" encoding = "UTF-8" ?>
<!--Interface properties are stored here to avoid hard-coded values.-->
<data>
    <items>
		<!--The string used to verify that a .txt file has the correct format.-->
		<item name="file-verification-stamp">I declare that the data in this *.txt file is correctly formatted for the N-Body Simulator.</item>

        <!--Body Properties that are randomly chosen when adding a body.-->
		<item name="body-colours">lightgreen,green,darkgreen,magenta,red,darkred,deepskyblue,blue,darkblue,orange,purple,yellow,gold,black,brown</item>
        <item name="body-names">Nevis,Lobiuvis,Ennuter,Vivis,Delia,Cerilia,Pagoria,Drigolara,Geonia,Coovis</item>

        <!--Step between values when using a QDoubleSpinBox.-->
        <item name="time-step-step">1.0</item>
        <item name="duration-step">10.0</item>
        <item name="mass-step">0.1</item>
        <item name="position-step">0.1</item>
        <item name="velocity-step">0.1</item>

        <!--Minimum allowed values.-->
        <item name="time-step-min">0.0</item>
        <item name="duration-min">0.0</item>
        <item name="mass-min">0.000001</item>
        <item name="position-min">-1000.0</item>
        <item name="velocity-min">-1000.0</item>

        <!--Maximum allowed values.-->
        <item name="time-step-max">500.0</item>
        <item name="duration-max">10000.0</item>
        <item name="mass-max">100.0</item>
        <item name="position-max">1000.0</item>
        <item name="velocity-max">1000.0</item>

        <!--Units for different parameters.-->
        <item name="time-unit">d</item>
        <item name="mass-unit">M*</item>
        <item name="position-unit">au</item>

        <!--Number of decimal places.-->
        <item name="time-dp">1</item>
        <item name="mass-dp">6</item>
        <item name="position-dp">6</item>
        <item name="velocity-dp">6</item>

        <!--Default animation values.-->
        <item name="frame-delay-default">3</item>
    </items>
</data>
